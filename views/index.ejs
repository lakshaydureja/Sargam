    .section-3 {
        display: grid;
        grid-template-columns: 1fr 1fr;
        gap: 20px;
        text-align: center;
    }

    .section-1__img,
    .section-2__img,
    .section-3__img {
        width: 60%;
        max-width: 100%;
        height: auto;
        border-radius: 8px;
        margin-left: auto;
        margin-right: auto;
        display: inline-block;
    }

    .img-container {
        width: 80%; /* Adjust the width for mobile */
        max-width: 100%;
        margin-left: auto;
        margin-right: auto;
    }

    .section-1__song-players,
    .section-2__song-players,
    .section-3__song-players {
        padding-top: 20%;
        display: inline-block;
        vertical-align: middle;
        width: calc(90% - 10px); /* Adjust the width and gap as needed */
        margin-right: 20px;
        text-align: left;
    }

    .song-player {
        background-color: #fffcf5;
        border-radius: 8px;
        overflow: hidden;
        transition: transform 0.3s ease-in-out;
        display: inline-block;
        width: 100%;
        margin-bottom: 20px; /* Adjust the gap between players */
        box-sizing: border-box;
        padding: 0px; /* Add padding for a better visual appearance */
    }

    .song-player:hover {
        transform: scale(1.05);
    }

    .song-player__audio {
        width: calc(100% - 20px);
        margin: 0 10px;
    }

    @media (max-width: 767px) {
        .img-container {
            width: 50%; /* Adjust the width for mobile */
            max-width: 100%;
            margin-left: auto;
            margin-right: auto;
        }

        .section-1__song-players,
        .section-2__song-players,
        .section-3__song-players {
            width: 100%;
            margin-right: 0;
        }
    }
    </style>
